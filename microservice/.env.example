# Teable API Configuration
BASE_URL=https://your-teable-domain.com/api
TEABLE_API_TOKEN=your_teable_api_token_here
TEABLE_TABLE_ID=your_table_id_here

# Polling Configuration
POLL_INTERVAL_SECONDS=5

# N8N Webhook URLs
N8N_WEBHOOK_URL=https://your-n8n-domain.com/webhook/selector
N8N_WEBHOOK_RECEIVED_URL=https://your-n8n-domain.com/webhook-test/app-received
N8N_WEBHOOK_ACCEPTED_URL=https://your-n8n-domain.com/webhook-test/approved

# Telegram Group Configuration
TELGRAM_GROUP_ID=your_telegram_group_id_here

# Telegram API Credentials (get from https://my.telegram.org/)
TELEGRAM_API_ID=your_telegram_api_id
TELEGRAM_API_HASH=your_telegram_api_hash
TELEGRAM_PHONE=your_phone_number_with_country_code
TELEGRAM_GROUP_HASH=your_telegram_group_hash

# Logging Configuration
# Available levels: DEBUG, INFO, WARNING, ERROR
# - DEBUG: Detailed information for debugging
# - INFO: General operational events (default)
# - WARNING: Warning messages for potential issues
# - ERROR: Error messages for critical problems
LOG_LEVEL=INFO
